//Example Comment
function a(b: i32): string
	let a = "hello"
	if(b == 1)
		a = "hello2"
	elif(b != 2)
		let x = 0
		while(x != 2)
			a = a + " world"
			x = x + 1
		end
	else
		//Empty
	end
	return a
end

println(a(0))
println(a(1))
println(a(2))

function println(input: string)
	write("/dev/stdout", input + "\n")
end
