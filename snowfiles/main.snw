//Example Comment
function a(b: i32): string
	let a = ["hello"]
	if(b == 1)
		a[0] = "hello2"
	elif(b != 2)
		let x = 0
		a[0] = "hello" + " world"
		while(x != 2)
			a[0] = a[0] + " world"
			x = x + 1
		end
	else
		//Empty
	end
	return a[0]
end

println(a(0))
println(a(1))
println(a(2))

function println(input: string)
	write("/dev/stdout", input + "\n")
end
